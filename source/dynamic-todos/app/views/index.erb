<div id="todo">
  <div class="toolbox">
    <form action="/add_todo" method="post">
      <input type="text" class="todo" placeholder="Put your todo item here!"/>
      <input type="submit" value="Add Todo"/>
      <!-- <button class="add" type="submit">Add Todo</button> -->
    </form>
  </div>

  <div class="todo_list">
  <div id="todo_template" >
    <div class="todo">
      <h2></h2>
      <ul>
        <li><a href="/add_todo/<%todo.id%>" class="delete">Delete</a></li>
        <li><a href="#" class="complete">Complete</a></li>
      </ul>
    </div>
  </div>
</div>
</div>

